{
  "manifest_version": 2,

  "name": "Quetzalext",
  "description": "Quicker than quetzals",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png"
  },
  "background": {
    "scripts": ["context_menu.js"]
  },
  "permissions": [
    "activeTab",
    "contextMenus",
    "https://ajax.googleapis.com/",
    "http://*/*",
    "https://*/*"
  ],
  "icons": {
    "16": "icon-small.png"
  }
}

